<Window x:Class="Projekt_philipmetzger.SearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_philipmetzger"
        mc:Ignorable="d"
        Title="SearchWindow" Height="300" Width="300">
    <Grid>
        <Image  x:Name="hotelImage" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.802,0.476"   />
        <Image x:Name="hotelStar" HorizontalAlignment="Left" Height="20" Margin="105,80,0,0" VerticalAlignment="Top" Width="20"/>
        <TextBox x:Name="hotelDescription" HorizontalAlignment="Left" Height="100" Margin="100,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="182" />

    </Grid>
</Window>
